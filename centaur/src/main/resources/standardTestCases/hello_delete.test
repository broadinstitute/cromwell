# Tests one of the simplest versions of intermediate file deletion

name: hello_delete
testFormat: workflowsuccess
backends: [Papiv2]

files {
  workflow: delete_intermediates/hello_delete.wdl
  options: delete_intermediates/delete_intermediates.options
}

metadata {
  workflowName: hello_delete
  status: Succeeded
  fileDeletionStatus: Succeeded
}

fileSystemCheck: "gcs"
outputExpectations: {
  "gs://cloud-cromwell-dev-self-cleaning/cromwell_execution/ci/hello_delete/<<UUID>>/call-hello/attempt-1/stdout": 1
  "gs://cloud-cromwell-dev-self-cleaning/cromwell_execution/ci/hello_delete/<<UUID>>/call-hello/attempt-1/stderr": 1
  "gs://cloud-cromwell-dev-self-cleaning/cromwell_execution/ci/hello_delete/<<UUID>>/call-hello/attempt-1/rc": 1
  "gs://cloud-cromwell-dev-self-cleaning/cromwell_execution/ci/hello_delete/<<UUID>>/call-hello/attempt-1/delete.txt": 0
  "gs://cloud-cromwell-dev-self-cleaning/cromwell_execution/ci/hello_delete/<<UUID>>/call-hello/attempt-1/keep.txt": 1
}
