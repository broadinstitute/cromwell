# Uses the Papiv2ParallelCompositeUploads backend which specifies a parallel composite upload threshold,
# but the workflow options are set to turn parallel composite uploads off.

name: parallel_composite_uploads_override_off
testFormat: workflowsuccess
backends: [Papiv2ParallelCompositeUploads]

files {
  workflow: parallel_composite_uploads/parallel_composite_uploads_on.wdl
  imports: [
    "parallel_composite_uploads/parallel_composite_uploads_lib.wdl"
  ]
  options: parallel_composite_uploads/parallel_composite_uploads_off.options
}

metadata {
  workflowName: composite_status
  status: Succeeded
  "outputs.composite_status.check.composite" = "false"
}
