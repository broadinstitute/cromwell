# This test is ignored as it expects a local file to exist, and we avoid that in centaur to allow cloud/local runs to just work.
# In order to facilitate running this test, a "make File" step could be added and the "file" input could be removed.
ignore: true
name: firstWorkflowCwl
testFormat: workflowsuccess
workflowType: CWL
workflowTypeVersion: v1.0

files {
  wdl: first_workflow/1st-workflow.cwl
  inputs: first_workflow/inputs.json
  imports: [
    first_workflow/arguments.cwl,
    first_workflow/tar-param.cwl,
    first_workflow/x.tar
  ]
}

metadata { }
